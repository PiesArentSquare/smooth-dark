	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91969D60"
			}
        },
        {
            "name": "String",
            "scope": [
                "string",
            ],
            "settings": {
                "foreground": "#ebc356"
            }
        },
        {
            "name": "String Embed Start/End",
            "scope": [
              "string.embedded.begin",
              "string.embedded.end",
              "punctuation.definition.template-expression",
              "punctuation.section.embedded"
            ],
            "settings": {
              "foreground": "#4A9AB4"
            }
        },
        {
            "name": "Default Embedded Color",
            "scope": [
              "punctuation.section.embedded.begin.js",
              "punctuation.section.embedded.end.js",
              "punctuation.section.embedded.begin.erb",
              "punctuation.section.embedded.end.erb",
              "source.elixir.embedded",
              "punctuation.separator",
              "punctuation.accessor",
              "meta.brace"
            ],
            "settings": {
              "foreground": "#91969D"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
              "foreground": "#BC61E9"
            }
        },
        {
            "name": "Language Constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#37E8E2"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
              "constant.character",
              "constant.other"
            ],
            "settings": {
              "foreground": "#5179ce"
            }
        },
        {
            "name": "Language Variable",
            "scope": "variable.language",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Keyword",
            "scope": [
              "keyword",
              "keyword.operator.logical",
              "keyword.operator.constructor"
            ],
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Keyword Operator",
            "scope": "keyword.operator",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Types",
            "scope": "storage.type",
            "settings": {
              "foreground": "#4A9AB4"
            }
        },
        {
            "name": "Class",
            "scope": [
              "entity.name.class",
              "entity.name.module",
              "entity.name.type",
              "storage.identifier",
              "support.class"
            ],
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Object Variable",
            "scope": [
              "variable.other.object",
              "variable.other.constant",
              "variable.other.global",
              "variable.other.readwrite.class",
              "variable.other.readwrite.instance",
              "variable.other.readwrite.batchfile",
              "variable.readwrite",
              "variable.readwrite.other.block"
            ],
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Other Variable",
            "scope": [
              "variable.other",
              "variable.other.property",
              "variable.other.block"
            ],
            "settings": {
              "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Inherited Class",
            "scope": "entity.other.inherited-class",
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Package Import",
            "scope": [
              "storage.modifier.import",
